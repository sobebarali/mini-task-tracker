{
	"name": "@mini-task-tracker/db",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./src/index.ts"
		},
		"./*": {
			"types": "./dist/*.d.ts",
			"default": "./src/*.ts"
		}
	},
	"scripts": {
		"build": "tsdown",
		"db:start": "docker compose up -d",
		"db:watch": "docker compose up",
		"db:stop": "docker compose stop",
		"db:down": "docker compose down"
	},
	"devDependencies": {
		"tsdown": "^0.15.5",
		"@types/node": "^22.13.11"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"dependencies": {
		"mongoose": "^8.14.0",
		"ioredis": "^5.4.2",
		"dotenv": "^17.2.2",
		"zod": "^4.1.11"
	}
}
